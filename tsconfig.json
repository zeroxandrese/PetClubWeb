{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es2015", "dom"],
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src", // Asegúrate de que baseUrl esté configurado correctamente si lo usas
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["src/**/*", "images.d.ts"],
    "exclude": ["node_modules"]
  }